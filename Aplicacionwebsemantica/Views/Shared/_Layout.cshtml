    <!--- ------------------------------------------------------------------------>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/bt/datatables/DataTables-1.10.18/css/dataTables.bootstrap4.css" rel="stylesheet" />
    @*<link href="~/Content/bt/bootstrap.min.css" rel="stylesheet" />*@
</head>
<body>
    <!-- Navbar -->
    <header class="header">
        <nav class="navbar navbar-default navbar-static-top">
            <div class="container">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navigation-nav">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand negrita-text" href="#">Aves</a>
                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="navigation-nav">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="active">@Html.ActionLink("Incio", "Index", "Home", new { @Class = "b" })</li>
                        <li>@Html.ActionLink("Todo", "ListarTodo", "Home", new { @Class = "b" })</li>
                        <li>@Html.ActionLink("Aves Familias", "ListarGeneral", "Home", new { @Class = "b" })</li>
                        <li>@Html.ActionLink("Parques Naturales", "ListaParques", "Home", new { @Class = "b" })</li>
                        <li>@Html.ActionLink("Avistadores", "ListarAvistador", "Home", new { @Class = "b" })</li>
                        <li>@Html.ActionLink("Familias", "ListarFamiliaUbicacion", "Home", new { @Class = "b" })</li>

                    </ul>
                </div><!-- /.navbar-collapse -->
            </div><!-- /.container -->
        </nav>
    </header>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Mi aplicación ASP.NET</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")



    <script src="~/Content/scripts/bootstrap.min.js"></script>
    <script src="~/Content/bt/js/jquery-3.6.0.min.js"></script>
    <!--<script src="~/Content/bt/bootstrap.min.js"></script>-->
    <script src="~/Content/bt/js/popper.min.js"></script>
    <script src="~/Content/bt/datatables/datatables.min.js"></script>

